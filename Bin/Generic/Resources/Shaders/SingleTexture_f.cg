float4 main(
	float2 vTexCoord : TEXCOORD0,
	uniform sampler2D TextureMap : TEXUNIT0,
	uniform half4 DiffuseColor)
{
    float4 textureColor = tex2D(TextureMap, vTexCoord);
	return(textureColor * DiffuseColor);
}

